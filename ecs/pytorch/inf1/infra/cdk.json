{
  "app": "npx ts-node --prefer-ts-exts bin/infra.ts",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "yarn.lock",
      "node_modules",
      "test"
    ]
  },
  "context": {
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "region": "us-east-1",
    "ecs-cluster": {
      "maxAzs": 3,
      "vpcId": "",
      "desiredCapacity": 3
    }
  }
}
